{
	"extends": [
		"eslint:recommended", "plugin:react/recommended"
	],
	"plugins": [
		"react"
	],
	"parser": "babel-eslint",

	"ecmaFeatures": {
		"jsx": true
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"no-console":"warn",
		"no-unused-vars": [0],
		"no-mixed-spaces-and-tabs":[0],
		"no-debugger":"warn",
		"no-class-assign":0,
		"no-cond-assign":0,
		"react/display-name":0,
		"react/prop-types":0,
		"react/no-find-dom-node":0
	},
	"globals":{
		"$":true	
	}
}
